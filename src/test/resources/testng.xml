        <class name="com.wildstartech.wfa.finance.CreditCardTest"/>
    </classes>
  </test>
  <!-- ********** wfa.location ********** -->
  <test name="wfa.location" parallel="false" preserve-order="true">
    <classes>
        <class name="com.wildstartech.wfa.location.GeoPointTest"/>
    </classes>
  </test>
  <test name="wfa.logistics">
